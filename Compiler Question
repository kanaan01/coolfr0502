#include<iostream>
using namespace std;
int sig(string a, int x,int y)
{ 
   if(a=="add") cout<<x+y<<endl;
   if(a=="sub") cout<<x-y<<endl; 
   if(a=="mul") cout<<x*y<<endl; 
   if(a=="div") cout<<x*1.0/y<<endl;
   if(a=="mod") cout<<x%y<<endl;
}
int main()
{
 string a;
 int x,y;
  for(int i=0;i<5;i++)
  {
    cin>>a>>x>>y;
    sig(a,x,y);
  }
}
