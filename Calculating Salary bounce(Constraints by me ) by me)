#include <iostream>
#include <string>
#include <iomanip>
using namespace std;
int main(void)
{
float   a;
string b;
cin>>a>>b;
if(b=="manager") cout<<setprecision(1) << fixed  << a*1.20;
   if(b=="programmer") cout<<setprecision(1) << fixed << a*1.15;
     if(b=="financial") cout<<setprecision(1) << fixed << a*1.10;
   if (b=="other") cout<<setprecision(1) << fixed << a*1.05;
return 0;
}
